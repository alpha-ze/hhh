{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    { "source": "/api", "destination": "/api/main" },
    { "source": "/api/:path*", "destination": "/api/main" },
    { "source": "/agent", "destination": "/api/agent" },
    { "source": "/agent/:path*", "destination": "/api/agent" }
  ]
}
