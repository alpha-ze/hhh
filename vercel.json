{
  "functions": {
    "api/main.py": {
      "runtime": "python3.9"
    },
    "api/agent.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    { "source": "/api", "destination": "/api/main" },
    { "source": "/api/(.*)", "destination": "/api/main" },
    { "source": "/agent", "destination": "/api/agent" },
    { "source": "/agent/(.*)", "destination": "/api/agent" }
  ]
}
